generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Game {
  id         String   @id @default(uuid())
  player1    String
  player2    String
  winner     String
  createdAt  DateTime @default(now())

  @@map("games")
}
